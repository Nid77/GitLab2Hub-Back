{
    "compilerOptions": {
      "lib": [
        "ES2020",
        "ESNext"
      ],
      "esModuleInterop": true,
      "module": "commonjs",
      "target": "ES2020",
      "outDir": ".build",
      "types": [
        "node"
      ],
      "paths": {
        "@/*": [
          "./src/*"
        ],
        "@/controller/*": [
          "./src/api/controller/*"
        ],
        "@/routes/*": [
          "./src/api/routes/*"
        ],
      }
    },
  }
  